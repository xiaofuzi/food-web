<% provide(:title,'All videos') %>
<div class=main-box>
<div class="row">
   
	<% @videos.each do |video| %>
	  <div class="span3 box">
	  <div class="inner-box">
	  	<%= link_to image_tag(video.cover,size: "180x100", :alt => video.title) , video %>
	  	<div class="bottom-box">
	  	    <p ><%= link_to video.title, video %></p>
	  	</div>
	  </div>
	  </div>
	<% end %>
	
</div>
<%= will_paginate %>
</div>
