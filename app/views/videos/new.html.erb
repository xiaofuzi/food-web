<% provide(:title,"Create video") %>
<div class="main-box">
<h1> Create a video</h1>
<div class="row">
	<div class="span6 offset4">
		<%= form_for(@video) do |v| %>

		  
           <div>
		   <%= v.label :videourl %>
		   <%= v.text_field :videourl %>
           </div>
           <div class="center">
		   <%= v.submit "Create a video", class: "btn  btn-primary" %>
		   </div>
		<% end %>
	</div>
</div>
</div>